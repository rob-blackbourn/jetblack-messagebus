{
  "distributor": {
    "address": "0.0.0.0",
    "port": 9001,
    "authentication": {
      "assemblyPath": null,
      "assemblyName": "JetBlack.MessageBus.Common",
      "typeName": "JetBlack.MessageBus.Common.Security.Authentication.NullAuthenticator",
      "args": []
    },
    "heartbeatInterval": "00:00:00",
    "prometheus": {
      "port": 9002
    },
    "ssl": {
      "isEnabled": false,
      "certFile": "/etc/ssl/certs/server.crt",
      "keyFile": "/etc/ssl/private/server.key",
      "storeLocation": "LocalMachine",
      "subjectName": "pebble.jetblack.net"
    },
    "sspi": {
      "isEnabled": false,
      "address": "0.0.0.0",
      "port":  9002
    },
    "allow": [
      "All"
    ],
    "deny": [
      "None"
    ],
    "isAuthorizationRequired": false,
    "isImpersonationAllowed": false,
    "isProxyAllowed": false,
    "useJsonLogger": false
  },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "IncludeScopes": true,
            "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fffffff "
        },
        "JsonConsole": {
            "Timestamp": "utc"
        }
    }
}