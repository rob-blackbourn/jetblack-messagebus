{
    "distributor": {
        "address": "0.0.0.0",
        "port": 9091,
        "authentication": {
            "assemblyPath": "%JETBLACK_MESSAGEBUS_ROOT%/extensions/authentication/src/JetBlack.MessageBus.JwtAuthentication/bin/Debug/netstandard2.1/JetBlack.MessageBus.JwtAuthentication.dll",
            "assemblyName": "JetBlack.MessageBus.JwtAuthentication",
            "typeName": "JetBlack.MessageBus.Common.Security.Authentication.JwtAuthenticator",
            "args": [
                "A secret of more than 15 characters"
            ]
        },
        "heartbeatInterval": "00:00:00",
        "sslConfig": {
            "isEnabled": true,
            "certFile": "%HOME%/.keys/server.crt",
            "keyFile": "%HOME%/.keys/server.key"
        },
        "allow": [
            "All"
        ],
        "deny": [
            "None"
        ],
        "isAuthorizationRequired": false
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "IncludeScopes": true
        }
    }
}