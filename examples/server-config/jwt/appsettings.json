{
    "distributor": {
        "address": "0.0.0.0",
        "port": 9001,
        "authentication": {
            "assemblyPath": "%JETBLACK_MESSAGEBUS_ROOT%/extensions/authentication/src/JetBlack.MessageBus.Extension.JwtAuthentication/bin/Debug/netstandard2.1/JetBlack.MessageBus.Extension.JwtAuthentication.dll",
            "assemblyName": "JetBlack.MessageBus.Extension.JwtAuthentication",
            "typeName": "JetBlack.MessageBus.Extension.JwtAuthenticator",
            "args": [
                "A secret of more than 15 characters"
            ]
        },
        "heartbeatInterval": "00:00:00",
        "sslConfig": {
            "isEnabled": true,
            "certFile": "%HOME%/.keys/server.crt",
            "keyFile": "%HOME%/.keys/server.key"
        },
        "prometheus": {
            "port": 9002,
            "isEnabled": true
        },
        "allow": [
            "None"
        ],
        "deny": [
            "All"
        ],
        "isAuthorizationRequired": true,
        "feedRoles": {
            "POSITION": {
                "isAuthorized": false,
                "allow": [
                    "All"
                ],
                "deny": [
                    "None"
                ]
            },
            "LOCAL.ALL": {
                "isAuthorized": false,
                "allow": [
                    "All"
                ],
                "deny": [
                    "None"
                ]
            },
            "LOCAL.ACCOUNT": {
                "isAuthorized": false,
                "allow": [
                    "All"
                ],
                "deny": [
                    "None"
                ]
            },
            "BBG": {
                "isAuthorized": true,
                "isImpersonationAllowed": true,
                "isProxyAllowed": true,
                "allow": [
                    "None"
                ],
                "deny": [
                    "All"
                ],
                "interactorRoles": {
                    "*": {
                        "aburrows": {
                            "allow": [
                                "Subscribe"
                            ],
                            "deny": [
                                "Publish",
                                "Notify",
                                "Authorize"
                            ]
                        },
                        "inazir": {
                            "allow": [
                                "Subscribe"
                            ],
                            "deny": [
                                "Publish",
                                "Notify",
                                "Authorize"
                            ]
                        },
                        "app_shared_bloomberg": {
                            "allow": [
                                "Publish",
                                "Notify",
                                "Authorize",
                                "Subscribe"
                            ],
                            "deny": [
                                "None"
                            ]
                        }
                    }
                }
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "IncludeScopes": true
        }
    }
}