{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build JetBlack.MessageBus.Distributor",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/core/src/JetBlack.MessageBus.Distributor/JetBlack.MessageBus.Distributor.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish JetBlack.MessageBus.Distributor",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/core/src/JetBlack.MessageBus.Distributor/JetBlack.MessageBus.Distributor.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch JetBlack.MessageBus.Distributor",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/core/src/JetBlack.MessageBus.Distributor/JetBlack.MessageBus.Distributor.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}